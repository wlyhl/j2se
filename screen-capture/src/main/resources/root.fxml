<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<VBox spacing="2" alignment="CENTER" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.jd.screen.cap.Controller"
      style="-fx-fill-height: true">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <Button onAction="#showRect">显示选框</Button>
    <Button onAction="#hideRect">隐藏选框</Button>
    <Button onAction="#setDirectory">储存目录</Button>

</VBox>
