<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<VBox spacing="2" alignment="CENTER" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.jd.hands.free.HandsFreeController"
      style="-fx-fill-height: true">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <Button onAction="#openWorkPath">打开工作目录</Button>
    <Button onAction="#saveConfig">保存配置</Button>

</VBox>
